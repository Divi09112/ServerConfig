server
{
	listen 0.0.0.0:80;
	server_name game.devclub.cse.iitd.ac.in tankassault.devclub.cse.iitd.ac.in;
	access_log /var/log/nginx/tank_assault_access.log;
	error_log /var/log/nginx/tank_assault_error.log;

	location / {
		proxy_pass http://localhost:3000;
		proxy_http_version 1.1;
		proxy_set_header Upgrade $http_upgrade;
	}
}
